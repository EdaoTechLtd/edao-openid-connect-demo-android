<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edao.oid.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="7" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.edao.oid.demo.activity.MyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.edao.oid.connect.activity.WebLoginActivity"></activity>
        <activity android:name="com.edao.oid.connect.activity.AppLoginActivity"></activity>
        
    </application>

</manifest>
